---
import { ViewTransitions } from "astro:transitions";
const { title, description } = Astro.props;
---

<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width" />
<title>{"Fisioterapia Neros" + title}</title>
<meta name="description" content={description} />

<meta property="og:title" content={title} />
<meta property="og:description" content={description} />
<meta
    property="og:image"
    content="https://www.fisioterapianeros.com/media/logo.png"
/>
<meta property="og:image:alt" content="Fisioterapia Neros logo" />
<meta property="og:site_name" content="Fisioterapia Neros" />
<meta property="og:locale" content="es_ESP" />
<meta property="og:url" content="https://fisioterapianeros.com" />
<meta property="og:type" content="website" />

<link rel="icon" href="https://www.fisioterapianeros.com/media/logo.png" />
<link rel="sitemap" href="/sitemap-index.xml" />

<script type="application/ld+json">
    {
        "@context": "https://schema.org",
        "@type": "Website",
        "headline": "Fisioterapia Neros",
        "image": ["https://www.fisioterapianeros.com/media/logo.png"],
        "author": {
            "@type": "Organization",
            "name": "Brodevs"
        },
        "publisher": {
            "@type": "Organization",
            "name": "Brodevs",
            "logo": {
                "@type": "ImageObject",
                "url": "/sponsors/brodevs.png"
            }
        },
        "datePublished": "2024-07-23",
        "dateModified": "2024-07-23"
    }
</script>
<ViewTransitions />
