    <Sponsors />
</Layout>

<style>
    h2 {
        text-align: center;
        padding-top: 50px;
        font-size: 50px;
        background: linear-gradient(90deg, var(--blueNeros), var(--pinkNeros));
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
    }

    @media (max-width: 767px) {
        h2 {
            font-size: 32px;
            width: 90%;
            margin: auto;
        }
    }

    .row {
        width: 90%;
        margin: auto;
        text-align: justify;
    }

    h3 {
        margin-top: 50px;
    }
</style>
